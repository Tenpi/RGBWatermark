{
  "name": "rgbwatermark",
  "version": "1.1.6",
  "description": "Protects your art against AI/machine learning theft with a customizable rainbow watermark.",
  "main": "dist/main.js",
  "scripts": {
    "start": "npm run clean && webpack && cross-env DEVELOPMENT=true electron dist/main.js",
    "clean": "del-cli ./dist",
    "build": "npm run clean ./build && webpack && electron-builder -p never",
    "rebuild": "npm rebuild --runtime=electron --target=23.2.0 --disturl=https://atom.io/download/atom-shell --abi=113",
    "release": "npm run clean ./build && webpack && cross-env CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder -mwl -p always"
  },
  "keywords": [
    "rainbow",
    "watermark",
    "art protection"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Tenpi/RGBWatermark"
  },
  "build": {
    "appId": "com.tenpi.rgbwatermark",
    "productName": "RGBWatermark",
    "icon": "./assets/icons/icon.png",
    "directories": {
      "buildResources": "assets",
      "output": "build"
    },
    "buildDependenciesFromSource": true,
    "mac": {
      "target": {
        "target": "dmg",
        "arch": "x64"
      },
      "category": "public.app-category.utilities"
    },
    "dmg": {
      "title": "${productName}",
      "background": "./assets/images/background.png",
      "window": {
        "width": 540,
        "height": 380
      }
    },
    "win": {
      "target": {
        "target": "nsis",
        "arch": "x64"
      }
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "uninstallDisplayName": "RGBWatermark",
      "createDesktopShortcut": "always",
      "installerSidebar": "./assets/images/installer.bmp",
      "uninstallerSidebar": "./assets/images/installer.bmp"
    },
    "linux": {
      "target": {
        "target": "appImage",
        "arch": "x64"
      },
      "category": "Utility"
    },
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "publish": {
      "provider": "github",
      "repo": "RGBWatermark",
      "owner": "Tenpi",
      "releaseType": "draft"
    }
  },
  "author": "Moepi",
  "license": "MIT",
  "devDependencies": {
    "@tenpi/tslint": "^1.1.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.5",
    "@types/sharp": "^0.31.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "del-cli": "^3.0.1",
    "electron": "^23.2.0",
    "electron-builder": "^23.0.2",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.1.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.1",
    "less": "^4.1.0",
    "less-loader": "^7.3.0",
    "mini-css-extract-plugin": "^1.3.5",
    "node-loader": "^1.0.3",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack": "^5.18.0",
    "webpack-cli": "^4.4.0",
    "webpack-obfuscator": "^3.5.1"
  },
  "dependencies": {
    "@electron/remote": "^2.0.1",
    "@ffmpeg/ffmpeg": "^0.11.6",
    "bootstrap": "^4.6.0",
    "canny-edge-detector": "^1.0.0",
    "electron-store": "^7.0.1",
    "electron-updater": "^4.3.5",
    "gif-encoder": "^0.7.2",
    "gif-frames": "github:tenpi/gif-frames#no-cumulative",
    "image-js": "^0.35.3",
    "image-pixels": "^2.2.2",
    "jszip": "^3.10.1",
    "magic-bytes.js": "^1.0.14",
    "mime": "^3.0.0",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.3",
    "react-dom": "^17.0.1",
    "react-dropzone": "^11.3.0",
    "react-router-dom": "^5.2.0",
    "react-router-hash-link": "^2.4.3",
    "react-slider": "^2.0.4"
  }
}
